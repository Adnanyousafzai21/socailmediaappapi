{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/posts/getPosts",
      "methods": ["GET","DELETE","POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
